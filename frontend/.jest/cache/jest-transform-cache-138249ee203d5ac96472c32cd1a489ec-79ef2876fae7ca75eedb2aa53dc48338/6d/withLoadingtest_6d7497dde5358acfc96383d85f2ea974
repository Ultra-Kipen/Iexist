_getJestObj().mock('../../src/components/LoadingIndicator', function () {
  return function MockLoadingIndicator() {
    return '로딩 인디케이터 컴포넌트';
  };
});
var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _withLoading = require("../../src/hoc/withLoading");
var _reactNative2 = require("react-native");
var _jsxRuntime = require("react/jsx-runtime");
function _getJestObj() {
  var _require = require("@jest/globals"),
    jest = _require.jest;
  _getJestObj = function _getJestObj() {
    return jest;
  };
  return jest;
}
describe('withLoading HOC', function () {
  var TestComponent = function TestComponent(_ref) {
    var message = _ref.message;
    return (0, _jsxRuntime.jsx)(_reactNative2.Text, {
      children: message
    });
  };
  var LoadingComponent = (0, _withLoading.withLoading)(TestComponent);
  test('isLoading이 true이면 로딩 인디케이터를 표시해야 함', function () {
    var result = (0, _reactNative.render)((0, _jsxRuntime.jsx)(LoadingComponent, {
      isLoading: true,
      message: "\uD14C\uC2A4\uD2B8 \uBA54\uC2DC\uC9C0"
    }));
    expect(result).toBeTruthy();
  });
  test('isLoading이 false이면 원래 컴포넌트를 렌더링해야 함', function () {
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(LoadingComponent, {
        isLoading: false,
        message: "\uD14C\uC2A4\uD2B8 \uBA54\uC2DC\uC9C0"
      })),
      getByText = _render.getByText;
    expect(getByText('테스트 메시지')).toBeTruthy();
  });
  test('isLoading prop이 제공되지 않으면 원래 컴포넌트를 렌더링해야 함', function () {
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(LoadingComponent, {
        message: "\uD14C\uC2A4\uD2B8 \uBA54\uC2DC\uC9C0"
      })),
      getByText = _render2.getByText;
    expect(getByText('테스트 메시지')).toBeTruthy();
  });
  test('HOC가 displayName을 올바르게 설정해야 함', function () {
    expect(LoadingComponent.displayName).toBe('withLoading(TestComponent)');
  });
  test('isLoading 상태가 변경될 때 컴포넌트가 적절하게 리렌더링되어야 함', function () {
    var _render3 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(LoadingComponent, {
        isLoading: true,
        message: "\uD14C\uC2A4\uD2B8 \uBA54\uC2DC\uC9C0"
      })),
      getByText = _render3.getByText,
      rerender = _render3.rerender;
    rerender((0, _jsxRuntime.jsx)(LoadingComponent, {
      isLoading: false,
      message: "\uD14C\uC2A4\uD2B8 \uBA54\uC2DC\uC9C0"
    }));
    expect(getByText('테스트 메시지')).toBeTruthy();
  });
});
