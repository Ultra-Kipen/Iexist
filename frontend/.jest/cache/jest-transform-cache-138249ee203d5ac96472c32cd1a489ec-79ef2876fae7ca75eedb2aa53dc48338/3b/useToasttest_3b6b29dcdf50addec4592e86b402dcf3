_getJestObj().mock('../../src/components/Toast', function () {
  return {
    ToastController: {
      show: jest.fn(),
      hide: jest.fn()
    }
  };
});
var _reactHooks = require("@testing-library/react-hooks");
var _useToast = require("../../src/hooks/useToast");
var _Toast = require("../../src/components/Toast");
function _getJestObj() {
  var _require = require("@jest/globals"),
    jest = _require.jest;
  _getJestObj = function _getJestObj() {
    return jest;
  };
  return jest;
}
describe('useToast', function () {
  beforeEach(function () {
    jest.clearAllMocks();
  });
  it('showToast 함수는 ToastController.show를 호출한다', function () {
    var _renderHook = (0, _reactHooks.renderHook)(function () {
        return (0, _useToast.useToast)();
      }),
      result = _renderHook.result;
    (0, _reactHooks.act)(function () {
      result.current.showToast('테스트 메시지');
    });
    expect(_Toast.ToastController.show).toHaveBeenCalledWith({
      message: '테스트 메시지',
      type: 'info',
      duration: 3000,
      position: 'bottom'
    });
  });
  it('showToast 함수는 type 매개변수를 적용한다', function () {
    var _renderHook2 = (0, _reactHooks.renderHook)(function () {
        return (0, _useToast.useToast)();
      }),
      result = _renderHook2.result;
    (0, _reactHooks.act)(function () {
      result.current.showToast('성공 메시지', 'success');
    });
    expect(_Toast.ToastController.show).toHaveBeenCalledWith(expect.objectContaining({
      message: '성공 메시지',
      type: 'success'
    }));
  });
  it('showToast 함수는 duration 매개변수를 적용한다', function () {
    var _renderHook3 = (0, _reactHooks.renderHook)(function () {
        return (0, _useToast.useToast)();
      }),
      result = _renderHook3.result;
    (0, _reactHooks.act)(function () {
      result.current.showToast('짧은 메시지', 'warning', 1000);
    });
    expect(_Toast.ToastController.show).toHaveBeenCalledWith(expect.objectContaining({
      message: '짧은 메시지',
      type: 'warning',
      duration: 1000
    }));
  });
  it('hideToast 함수는 ToastController.hide를 호출한다', function () {
    var _renderHook4 = (0, _reactHooks.renderHook)(function () {
        return (0, _useToast.useToast)();
      }),
      result = _renderHook4.result;
    (0, _reactHooks.act)(function () {
      result.current.hideToast();
    });
    expect(_Toast.ToastController.hide).toHaveBeenCalled();
  });
  it('반환된 함수들은 메모이제이션 된다', function () {
    var _renderHook5 = (0, _reactHooks.renderHook)(function () {
        return (0, _useToast.useToast)();
      }),
      result = _renderHook5.result,
      rerender = _renderHook5.rerender;
    var initialShowToast = result.current.showToast;
    var initialHideToast = result.current.hideToast;
    rerender();
    expect(result.current.showToast).toBe(initialShowToast);
    expect(result.current.hideToast).toBe(initialHideToast);
  });
});
