var _reactHooks = require("@testing-library/react-hooks");
var _useModal = require("../../src/hooks/useModal");
describe('useModal', function () {
  it('기본 상태는 false(모달 숨김)이다', function () {
    var _renderHook = (0, _reactHooks.renderHook)(function () {
        return (0, _useModal.useModal)();
      }),
      result = _renderHook.result;
    expect(result.current.isVisible).toBe(false);
  });
  it('initialState 매개변수로 초기 상태를 설정할 수 있다', function () {
    var _renderHook2 = (0, _reactHooks.renderHook)(function () {
        return (0, _useModal.useModal)(true);
      }),
      result = _renderHook2.result;
    expect(result.current.isVisible).toBe(true);
  });
  it('showModal 함수는 isVisible을 true로 설정한다', function () {
    var _renderHook3 = (0, _reactHooks.renderHook)(function () {
        return (0, _useModal.useModal)();
      }),
      result = _renderHook3.result;
    (0, _reactHooks.act)(function () {
      result.current.showModal();
    });
    expect(result.current.isVisible).toBe(true);
  });
  it('hideModal 함수는 isVisible을 false로 설정한다', function () {
    var _renderHook4 = (0, _reactHooks.renderHook)(function () {
        return (0, _useModal.useModal)(true);
      }),
      result = _renderHook4.result;
    (0, _reactHooks.act)(function () {
      result.current.hideModal();
    });
    expect(result.current.isVisible).toBe(false);
  });
  it('toggleModal 함수는 isVisible 상태를 토글한다', function () {
    var _renderHook5 = (0, _reactHooks.renderHook)(function () {
        return (0, _useModal.useModal)(false);
      }),
      result = _renderHook5.result;
    (0, _reactHooks.act)(function () {
      result.current.toggleModal();
    });
    expect(result.current.isVisible).toBe(true);
    (0, _reactHooks.act)(function () {
      result.current.toggleModal();
    });
    expect(result.current.isVisible).toBe(false);
  });
  it('반환된 함수들은 메모이제이션 된다', function () {
    var _renderHook6 = (0, _reactHooks.renderHook)(function () {
        return (0, _useModal.useModal)();
      }),
      result = _renderHook6.result,
      rerender = _renderHook6.rerender;
    var initialShowModal = result.current.showModal;
    var initialHideModal = result.current.hideModal;
    var initialToggleModal = result.current.toggleModal;
    rerender();
    expect(result.current.showModal).toBe(initialShowModal);
    expect(result.current.hideModal).toBe(initialHideModal);
    expect(result.current.toggleModal).toBe(initialToggleModal);
  });
});
