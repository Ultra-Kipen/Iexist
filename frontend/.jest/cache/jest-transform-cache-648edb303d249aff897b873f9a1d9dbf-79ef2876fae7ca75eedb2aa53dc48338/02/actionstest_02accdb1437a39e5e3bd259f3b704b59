var _actions = require("../../src/store/actions");
var _types = require("../../src/store/types");
describe('액션 생성자 함수', function () {
  test('setAuthenticated 액션 생성자가 올바른 액션을 반환한다', function () {
    var action = _actions.actions.setAuthenticated(true);
    expect(action).toEqual({
      type: _types.ActionType.SET_AUTHENTICATED,
      payload: true
    });
  });
  test('setUser 액션 생성자가 올바른 액션을 반환한다', function () {
    var user = {
      id: 1,
      username: 'test',
      email: 'test@example.com'
    };
    var action = _actions.actions.setUser(user);
    expect(action).toEqual({
      type: _types.ActionType.SET_USER,
      payload: user
    });
  });
  test('addNotification 액션 생성자가 올바른 액션을 반환한다', function () {
    var notification = {
      id: '1',
      content: 'Test notification',
      notificationType: 'system',
      isRead: false,
      createdAt: '2024-01-01T00:00:00Z'
    };
    var action = _actions.actions.addNotification(notification);
    expect(action).toEqual({
      type: _types.ActionType.ADD_NOTIFICATION,
      payload: notification
    });
  });
  test('removeNotification 액션 생성자가 올바른 액션을 반환한다', function () {
    var action = _actions.actions.removeNotification('1');
    expect(action).toEqual({
      type: _types.ActionType.REMOVE_NOTIFICATION,
      payload: '1'
    });
  });
  test('clearNotifications 액션 생성자가 올바른 액션을 반환한다', function () {
    var action = _actions.actions.clearNotifications();
    expect(action).toEqual({
      type: _types.ActionType.CLEAR_NOTIFICATIONS
    });
  });
  test('setTheme 액션 생성자가 올바른 액션을 반환한다', function () {
    var action = _actions.actions.setTheme('dark');
    expect(action).toEqual({
      type: _types.ActionType.SET_THEME,
      payload: 'dark'
    });
  });
  test('setLoading 액션 생성자가 올바른 액션을 반환한다', function () {
    var action = _actions.actions.setLoading(true);
    expect(action).toEqual({
      type: _types.ActionType.SET_LOADING,
      payload: true
    });
  });
  test('setError 액션 생성자가 올바른 액션을 반환한다', function () {
    var action = _actions.actions.setError('오류 메시지');
    expect(action).toEqual({
      type: _types.ActionType.SET_ERROR,
      payload: '오류 메시지'
    });
  });
  test('clearError 액션 생성자가 올바른 액션을 반환한다', function () {
    var action = _actions.actions.clearError();
    expect(action).toEqual({
      type: _types.ActionType.CLEAR_ERROR
    });
  });
  test('resetState 액션 생성자가 올바른 액션을 반환한다', function () {
    var action = _actions.actions.resetState();
    expect(action).toEqual({
      type: _types.ActionType.RESET_STATE
    });
  });
});
