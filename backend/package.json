{
  "name": "iexist-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node index.ts",
    "dev": "cross-env NODE_ENV=development nodemon index.ts",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit --no-cache",
    "test:watch": "cross-env NODE_ENV=test jest --watch --no-cache",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --no-cache",
    "test:emotion": "cross-env NODE_ENV=test jest --no-cache tests/integration/emotionLog.test.ts --detectOpenHandles --forceExit",
    "test:clear": "jest --clearCache",
    "db:test": "ts-node utils/testDb.ts",
    "db:check": "ts-node utils/checkDb.ts",
    "build": "tsc"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.3",
    "rate-limiter-flexible": "^5.0.0",
    "sequelize": "^6.37.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.17.3",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4"
  }
}
